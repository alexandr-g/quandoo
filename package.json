{
  "name": "alexander-quandoo-candidate-task",
  "version": "1.0.0",
  "description": "Quandoo candidate task",
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "main": "index.js",
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": ["<rootDir>/__tests__/__mocks__/"],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "moduleFileExtensions": ["js", "json"]
  },
  "scripts": {
    "test": "node_modules/.bin/jest && codecov",
    "lint": "eslint src/**/*.js webpack.config.js",
    "build": "./node_modules/.bin/webpack --config webpack.config.js --env.TARGET_ENV=production",
    "watch": "npm run build -- --watch",
    "dev":
      "webpack-dev-server --config webpack.config.js --env.TARGET_ENV=development"
  },
  "author": "Alexander Grischuk",
  "license": "MIT",
  "dependencies": {
    "react": "16.4.0",
    "react-dom": "16.3.2",
    "react-input-autosize": "2.2.1",
    "react-select": "^1.0.0-rc.3",
    "recompose": "0.27.1",
    "styled-components": "3.3.3"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-core": "6.26.3",
    "babel-jest": "23.2.0",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "codecov": "3.0.2",
    "css-loader": "0.28.11",
    "cypress": "3.0.2",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.0.1",
    "eslint-config-standard": "11.0.0",
    "eslint-config-standard-jsx": "5.0.0",
    "eslint-config-standard-react": "6.0.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-cypress": "2.0.1",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.10.0",
    "eslint-plugin-standard": "3.1.0",
    "jest": "23.3.0",
    "react-test-renderer": "16.3.2",
    "style-loader": "0.21.0",
    "webpack": "4.15.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  }
